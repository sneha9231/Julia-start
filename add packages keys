#created doc
using PkgTemplates

# Create a Template object
TMP = Template(
    user="sneha9231",
    authors="Sneha Pandey",
    plugins=[
        License(; name="MIT"),
        TravisCI(),
    ], 
)

# Generate a project using the template
TMP("MyDoc")

